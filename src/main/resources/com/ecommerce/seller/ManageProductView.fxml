<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ecommerce.seller.ManageProductsController" spacing="10" alignment="CENTER">
    <ListView fx:id="productList" />
    <Button text="Add New Product" fx:id="addProductButton" />
    <Button text="Edit Product" fx:id="editProductButton" />
    <Button text="Delete Product" fx:id="deleteProductButton" />
</VBox>
